<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/btn_my_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나의 게시글"/>
    <Button
        android:id="@+id/btn_my_comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나의 댓글"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_board"/>
    <Button
        android:id="@+id/btn_my_bookmark_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내가누른 북마크 게시글"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_comment"/>
    <Button
        android:id="@+id/btn_my_like_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내가누른 좋아요 게시글"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_bookmark_board"/>
    <Button
        android:id="@+id/btn_my_bookmark_comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내가누른 북마크 댓글"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_like_board"/>
    <Button
        android:id="@+id/btn_my_like_comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내가누른 좋아요 댓글"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_bookmark_comment"/>

</androidx.constraintlayout.widget.ConstraintLayout>